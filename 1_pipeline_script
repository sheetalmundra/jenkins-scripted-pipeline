node{
	stage('Build'){
		sh 'echo Hello world we are building!!'
	}
	
	stage('Test'){
		sh 'echo Hello world we are testing!!'
	}

	stage('Deliver'){
		sh 'echo packge ready for archive!!'
	}
	stage('Release'){
		sh 'echo package reagy to release!!'
	}

	stage('Deploy'){
		sh 'echo we are deploying'
	}
}

